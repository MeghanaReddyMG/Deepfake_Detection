{
  "version": 2,
  "builds": [
    {
      "src": "wsgi_lite.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "wsgi_lite.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production",
    "FLASK_DEBUG": "False",
    "PYTHONPATH": "/vercel/path0"
  },
  "functions": {
    "wsgi_lite.py": {
      "maxDuration": 30
    }
  },
  "installCommand": "pip install -r requirements-vercel.txt"
}